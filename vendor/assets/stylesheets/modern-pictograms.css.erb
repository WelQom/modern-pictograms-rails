@font-face {
    font-family: 'modern_pictogramsnormal';
    src: url('modernpics.eot');
    src: url('modernpics.eot?#iefix') format('embedded-opentype'),
         url('modernpics.woff') format('woff'),
         url('modernpics.ttf') format('truetype'),
         url('modernpics.svg#modern_pictogramsnormal') format('svg');
    font-weight: normal;
    font-style: normal;

}

@font-face {
  font-family: "FontAwesome";
  src: url('<%= asset_path('fontawesome-webfont.eot') %>');
  src: url('<%= asset_path('fontawesome-webfont.eot?#iefix') %>') format('eot'), 
       url('<%= asset_path('fontawesome-webfont.woff') %>') format('woff'),
       url('<%= asset_path('fontawesome-webfont.ttf') %>') format('truetype'),
       url('<%= asset_path('fontawesome-webfont.svg#FontAwesome') %>') format('svg');
  font-weight: normal;
  font-style: normal;
}

/*  Font Awesome styles
    ------------------------------------------------------- */
[class^="mpicon-"]:before, [class*=" mpicon-"]:before {
  font-family: modern_pictogramsnormal;
  font-weight: normal;
  font-style: normal;
  display: inline-block;
  text-decoration: inherit;
}
/* hide default bootstrap backgrounds so that they don't overlap with font awesome */
[class^="mpicon-"], [class*=" mpicon-"], .mpicon-white, .nav > .active > a > [class^="mpicon-"], .nav > .active > a > [class=" mpicon-"], .dropdown-menu > li > a:hover > [class^="mpicon-"], .dropdown-menu > li > a:hover > [class=" mpicon-"], .dropdown-menu > .active > a > [class^="mpicon-"], .dropdown-menu > .active > a > [class*=" mpicon-"] {
  background: none;
}
a [class^="mpicon-"], a [class*=" mpicon-"] {
  display: inline-block;
  text-decoration: inherit;
}
/* makes the font % larger relative to the icon container */
.mpicon-large:before {
  vertical-align: top;
  font-size: 1.3333333333333333em;
}
.btn [class^="mpicon-"], .btn [class*=" mpicon-"] {
  /* keeps button heights with and without icons the same */

  display: inline;
}
li [class^="mpicon-"], li [class*=" mpicon-"] {
  display: inline-block;
  width: 1.25em;
  text-align: center;
}
li .mpicon-large[class^="mpicon-"], li .mpicon-large[class*=" mpicon-"] {
  /* . increased font size for mpicon-large * . width */

  width: 1.5625em;
}
li[class^="mpicon-"], li[class*=" mpicon-"] {
  margin-left: ;
  list-style-type: none;
}
li[class^="mpicon-"]:before, li[class*=" mpicon-"]:before {
  text-indent: -em;
  text-align: center;
}
li[class^="mpicon-"].mpicon-large:before, li[class*=" mpicon-"].mpicon-large:before {
  text-indent: -0.75em;
}
